<p id="notice"><%= notice %></p>

<h1>Book Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @book_types.each do |book_type| %>
      <tr>
        <td><%= link_to 'Show', book_type %></td>
        <td><%= link_to 'Edit', edit_book_type_path(book_type) %></td>
        <td><%= link_to 'Destroy', book_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Book Type', new_book_type_path %>
